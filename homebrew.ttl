# Formula Properties
brew:formulaName a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:label "formula name" ;
    rdfs:comment "The name of the Homebrew formula" ;
    rdfs:domain brew:Formula ;
    rdfs:range xsd:string ;
    IAO:0000115 "The name of the Homebrew formula" .

brew:license a owl:DatatypeProperty ;
    rdfs:label "license" ;
    rdfs:comment "The software license" ;
    rdfs:domain brew:Formula ;
    rdfs:range xsd:string ;
    IAO:0000115 "The software license" .

brew:revision a owl:DatatypeProperty ;
    rdfs:label "revision" ;
    rdfs:comment "The formula revision number" ;
    rdfs:domain brew:Formula ;
    rdfs:range xsd:int ;
    IAO:0000115 "The formula revision number" .

brew:versionScheme a owl:DatatypeProperty ;
    rdfs:label "version scheme" ;
    rdfs:comment "The version numbering scheme" ;
    rdfs:domain brew:Formula ;
    rdfs:range xsd:int ;
    IAO:0000115 "The version numbering scheme" .

brew:kegOnly a owl:DatatypeProperty ;
    rdfs:label "keg only" ;
    rdfs:comment "Whether the formula is keg-only (not linked)" ;
    rdfs:domain brew:Formula ;
    rdfs:range xsd:boolean ;
    IAO:0000115 "Whether the formula is keg-only (not linked)" .

brew:deprecated a owl:DatatypeProperty ;
    rdfs:label "deprecated" ;
    rdfs:comment "Whether the formula is deprecated" ;
    rdfs:domain brew:Formula ;
    rdfs:range xsd:boolean ;
    IAO:0000115 "Whether the formula is deprecated" .

brew:disabled a owl:DatatypeProperty ;
    rdfs:label "disabled" ;
    rdfs:comment "Whether the formula is disabled" ;
    rdfs:domain brew:Formula ;
    rdfs:range xsd:boolean ;
    IAO:0000115 "Whether the formula is disabled" .

brew:deprecationDate a owl:DatatypeProperty ;
    rdfs:label "deprecation date" ;
    rdfs:comment "The date when the formula was deprecated" ;
    rdfs:domain brew:Formula ;
    rdfs:range xsd:date ;
    IAO:0000115 "The date when the formula was deprecated" .

brew:disableDate a owl:DatatypeProperty ;
    rdfs:label "disable date" ;
    rdfs:comment "The date when the formula was disabled" ;
    rdfs:domain brew:Formula ;
    rdfs:range xsd:date ;
    IAO:0000115 "The date when the formula was disabled" .

brew:deprecationReason a owl:DatatypeProperty ;
    rdfs:label "deprecation reason" ;
    rdfs:comment "The reason for deprecation" ;
    rdfs:domain brew:Formula ;
    rdfs:range xsd:string ;
    IAO:0000115 "The reason for deprecation" .

brew:disableReason a owl:DatatypeProperty ;
    rdfs:label "disable reason" ;
    rdfs:comment "The reason for disabling" ;
    rdfs:domain brew:Formula ;
    rdfs:range xsd:string ;
    IAO:0000115 "The reason for disabling" .

