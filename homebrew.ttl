    rdfs:label "build depends" ;
    rdfs:comment "Build-time dependency" ;
    IAO:0000115 "Build-time dependency" .

brew:optionalDepends a owl:ObjectProperty ;
    rdfs:subPropertyOf brew:hasDependency ;
    rdfs:label "optional depends" ;
    rdfs:comment "Optional dependency" ;
    IAO:0000115 "Optional dependency" .

brew:recommendedDepends a owl:ObjectProperty ;
    rdfs:subPropertyOf brew:hasDependency ;
    rdfs:label "recommended depends" ;
    rdfs:comment "Recommended dependency" ;
    IAO:0000115 "Recommended dependency" .

brew:runtimeDepends a owl:ObjectProperty ;
    rdfs:subPropertyOf brew:hasDependency ;
    rdfs:label "runtime depends" ;
    rdfs:comment "Runtime dependency" ;
    IAO:0000115 "Runtime dependency" .

# Common Taps
brew:homebrew-core a brew:Tap ;
    rdfs:label "homebrew/core" ;
    rdfs:comment "The main Homebrew tap containing core formulae" ;
    foaf:homepage <https://github.com/Homebrew/homebrew-core> .

brew:homebrew-cask a brew:Tap ;
    rdfs:label "homebrew/cask" ;
    rdfs:comment "The main Homebrew cask tap for macOS applications" ;
    foaf:homepage <https://github.com/Homebrew/homebrew-cask> .

brew:homebrew-cask-fonts a brew:Tap ;
    rdfs:label "homebrew/cask-fonts" ;
    rdfs:comment "Homebrew cask tap for fonts" ;
    foaf:homepage <https://github.com/Homebrew/homebrew-cask-fonts> .

# Common dependency types
brew:BuildDependency a owl:Class ;
    rdfs:subClassOf brew:Dependency ;
    rdfs:label "Build Dependency" ;
    rdfs:comment "Dependency needed only at build time" ;
    IAO:0000115 "A dependency that is required only during the build process of a Homebrew formula and is not needed at runtime." .

brew:OptionalDependency a owl:Class ;
    rdfs:subClassOf brew:Dependency ;
    rdfs:label "Optional Dependency" ;
    rdfs:comment "Optional dependency that adds extra functionality" ;
    IAO:0000115 "A dependency that provides additional features or functionality but is not required for basic operation of a Homebrew formula." .

brew:RecommendedDependency a owl:Class ;
    rdfs:subClassOf brew:Dependency ;
    rdfs:label "Recommended Dependency" ;
    rdfs:comment "Recommended dependency (deprecated)" ;
    IAO:0000115 "A dependency that was previously recommended for optimal functionality but is now deprecated in favor of other dependency types." .

brew:RuntimeDependency a owl:Class ;
    rdfs:subClassOf brew:Dependency ;
    rdfs:label "Runtime Dependency" ;
    rdfs:comment "Dependency needed at runtime" ;
    IAO:0000115 "A dependency that is required for the proper execution and operation of a Homebrew formula after installation." .

# Cardinality Restrictions for Homebrew Classes
brew:Formula rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty brew:formulaName ;
    owl:cardinality 1
] .

brew:Formula rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty brew:hasStable ;
    owl:qualifiedCardinality 1 ;
    owl:onClass brew:StableSpec
] .

brew:Formula rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty brew:hasHead ;
    owl:maxQualifiedCardinality 1 ;
    owl:onClass brew:HeadSpec
] .

brew:Cask rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty brew:caskName ;
    owl:cardinality 1
] .

brew:Cask rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty brew:token ;
    owl:cardinality 1
] .
