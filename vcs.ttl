# Version Control System Ontology
@prefix vcs: <https://packagegraph.github.io/ontology/vcs#> .
@prefix pkg: <https://packagegraph.github.io/ontology/core#> .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix spdx: <http://spdx.org/spdxdocs/spdx-v2.2.1#> .
@prefix sioc: <http://rdfs.org/sioc/ns#> .
@prefix IAO: <http://purl.obolibrary.org/obo/IAO_> .


# Ontology Declaration
<https://packagegraph.github.io/ontology/vcs#> a owl:Ontology ;
    rdfs:label "Version Control System Ontology" ;
    rdfs:comment "Ontology for representing source code repositories, commits, branches, tags, and version control metadata" ;
    dcterms:created "2025-09-05"^^xsd:date ;
    dcterms:creator "PackageGraph Project" ;
    dcterms:title "Version Control System Ontology" ;
    dcterms:description "A comprehensive ontology for modeling version control systems, source code repositories, commits, branches, tags, and related metadata including file changes, authorship, and repository hosting platforms." ;
    dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
    owl:versionInfo "1.0.0" ;
    owl:imports <core.ttl> .

# VCS Classes
vcs:Repository a owl:Class ;
    rdfs:subClassOf pkg:Repository ;
    rdfs:label "Source Code Repository" ;
    rdfs:comment "A version control repository containing source code" ;
    IAO:0000115 "A version control repository that stores and tracks changes to source code and related project files over time." .

vcs:Commit a owl:Class ;
    rdfs:subClassOf prov:Entity ;
    rdfs:label "Commit" ;
    rdfs:comment "A commit in a version control system" ;
    IAO:0000115 "A snapshot of changes to files in a version control repository, representing a specific point in the project's history with associated metadata such as author, timestamp, and message." .

vcs:Branch a owl:Class ;
    rdfs:subClassOf prov:Entity ;
    rdfs:label "Branch" ;
    rdfs:comment "A branch in a version control repository" ;
    IAO:0000115 "A parallel line of development in a version control repository that allows developers to work on features or fixes independently from the main codebase." .

vcs:Tag a owl:Class ;
    rdfs:subClassOf prov:Entity ;
    rdfs:label "Tag" ;
    rdfs:comment "A tag pointing to a specific commit or release" ;
    IAO:0000115 "A named reference pointing to a specific commit, typically used to mark release points or important milestones in a project's history." .

vcs:Reference a owl:Class ;
    rdfs:subClassOf prov:Entity ;
    rdfs:label "VCS Reference" ;
    rdfs:comment "A generic reference in version control (HEAD, refs, etc.)" ;
    IAO:0000115 "A named pointer to a specific commit or object in a version control system, such as HEAD, branch references, or remote tracking branches." .

vcs:File a owl:Class ;
    rdfs:subClassOf prov:Entity ;
    rdfs:label "File" ;
    rdfs:comment "A file in a repository or package" ;
    IAO:0000115 "A file contained within a version control repository, including source code, documentation, configuration files, and other project assets." .

vcs:Diff a owl:Class ;
    rdfs:subClassOf owl:Thing ;
    rdfs:label "Diff" ;
    rdfs:comment "A diff or patch between two versions of files" ;
    IAO:0000115 "A representation of the differences between two versions of files or commits, showing additions, deletions, and modifications in a structured format." .

vcs:PullRequest a owl:Class ;
    rdfs:subClassOf prov:Activity ;
    rdfs:label "Pull Request" ;
    rdfs:comment "A pull/merge request for code changes" ;
    IAO:0000115 "A request to merge changes from one branch into another, typically used for code review and collaborative development workflows." .

vcs:Issue a owl:Class ;
    rdfs:subClassOf owl:Thing ;
    rdfs:label "Issue" ;
    rdfs:comment "An issue or bug report in a repository" ;
    IAO:0000115 "A tracked item representing a bug report, feature request, or other task associated with a software project repository." .

# VCS Properties
vcs:vcsType a owl:DatatypeProperty ;
    rdfs:label "VCS type" ;
    rdfs:comment "Type of version control system (git, svn, hg, bzr, etc.)" ;
    rdfs:domain vcs:Repository ;
    rdfs:range xsd:string ;
    IAO:0000115 "Identifies the type of version control system used by the repository, such as Git, Subversion, or Mercurial." .

vcs:remoteUrl a owl:DatatypeProperty ;
    rdfs:label "remote URL" ;
    rdfs:comment "URL of the remote repository" ;
    rdfs:domain vcs:Repository ;
    rdfs:range xsd:anyURI ;
    IAO:0000115 "The URL of a remote repository that this repository is connected to for synchronization purposes." .

vcs:cloneUrl a owl:DatatypeProperty ;
    rdfs:label "clone URL" ;
    rdfs:comment "URL for cloning the repository" ;
    rdfs:domain vcs:Repository ;
    rdfs:range xsd:anyURI ;
    IAO:0000115 "The URL that can be used to create a local copy of the repository using version control tools." .

vcs:defaultBranch a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:label "default branch" ;
    rdfs:comment "Name of the default branch" ;
    rdfs:domain vcs:Repository ;
    rdfs:range xsd:string ;
    IAO:0000115 "The name of the primary branch that serves as the default when cloning or accessing the repository." .

# Commit Properties
vcs:commitHash a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:label "commit hash" ;
    rdfs:comment "Hash identifier of the commit (SHA1, SHA256, etc.)" ;
    rdfs:domain vcs:Commit ;
    rdfs:range xsd:string ;
    IAO:0000115 "The cryptographic hash that uniquely identifies a commit in the version control system." .

vcs:commitMessage a owl:DatatypeProperty ;
    rdfs:label "commit message" ;
    rdfs:comment "Commit message describing the changes" ;
    rdfs:domain vcs:Commit ;
    rdfs:range xsd:string ;
    IAO:0000115 "A human-readable description of the changes made in the commit, typically including a summary and optional detailed explanation." .

vcs:commitTimestamp a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:label "commit timestamp" ;
    rdfs:comment "Timestamp when the commit was created" ;
    rdfs:domain vcs:Commit ;
    rdfs:range xsd:dateTime ;
    IAO:0000115 "The date and time when the commit was created and added to the repository." .

vcs:authorTimestamp a owl:DatatypeProperty ;
    rdfs:label "author timestamp" ;
    rdfs:comment "Timestamp when the changes were authored" ;
    rdfs:domain vcs:Commit ;
    rdfs:range xsd:dateTime ;
    IAO:0000115 "The date and time when the changes in the commit were originally authored, which may differ from the commit timestamp." .

# Branch Properties
vcs:branchName a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:label "branch name" ;
    rdfs:comment "Name of the branch" ;
    rdfs:domain vcs:Branch ;
    rdfs:range xsd:string ;
    IAO:0000115 "The human-readable name identifier of a branch in the version control repository." .

vcs:isProtected a owl:DatatypeProperty ;
    rdfs:label "is protected" ;
    rdfs:comment "Whether the branch is protected from direct pushes" ;
    rdfs:domain vcs:Branch ;
    rdfs:range xsd:boolean ;
    IAO:0000115 "Indicates whether the branch has protection rules that prevent direct pushes and require pull requests or other approval processes." .

# Tag Properties
vcs:tagName a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:label "tag name" ;
    rdfs:comment "Name of the tag" ;
    rdfs:domain vcs:Tag ;
    rdfs:range xsd:string ;
    IAO:0000115 "The human-readable name identifier of a tag in the version control repository." .

vcs:tagMessage a owl:DatatypeProperty ;
    rdfs:label "tag message" ;
    rdfs:comment "Message associated with the tag" ;
    rdfs:domain vcs:Tag ;
    rdfs:range xsd:string ;
    IAO:0000115 "A human-readable message describing the purpose or significance of the tag, typically used for release notes." .

vcs:isAnnotated a owl:DatatypeProperty ;
    rdfs:label "is annotated" ;
    rdfs:comment "Whether the tag is annotated" ;
    rdfs:domain vcs:Tag ;
    rdfs:range xsd:boolean ;
    IAO:0000115 "Indicates whether the tag includes additional metadata such as a message, tagger information, and timestamp." .

# File Properties
vcs:filePath a owl:DatatypeProperty ;
    rdfs:label "file path" ;
    rdfs:comment "Path of the file within the repository" ;
    rdfs:domain vcs:File ;
    rdfs:range xsd:string ;
    IAO:0000115 "The relative path and filename of the file within the repository's directory structure." .

vcs:fileHash a owl:DatatypeProperty ;
    rdfs:label "file hash" ;
    rdfs:comment "Content hash of the file" ;
    rdfs:domain vcs:File ;
    rdfs:range xsd:string ;
    IAO:0000115 "A cryptographic hash of the file's content used for integrity verification and change detection." .

vcs:fileSize a owl:DatatypeProperty ;
    rdfs:label "file size" ;
    rdfs:comment "Size of the file in bytes" ;
    rdfs:domain vcs:File ;
    rdfs:range xsd:long ;
    IAO:0000115 "The size of the file content measured in bytes." .

vcs:mimeType a owl:DatatypeProperty ;
    rdfs:label "MIME type" ;
    rdfs:comment "MIME type of the file" ;
    rdfs:domain vcs:File ;
    rdfs:range xsd:string ;
    IAO:0000115 "The MIME type that identifies the format and nature of the file content." .

vcs:encoding a owl:DatatypeProperty ;
    rdfs:label "encoding" ;
    rdfs:comment "Character encoding of the file" ;
    rdfs:domain vcs:File ;
    rdfs:range xsd:string ;
    IAO:0000115 "The character encoding format used for the file content, such as UTF-8, ASCII, or ISO-8859-1." .

# Object Properties
vcs:hasCommit a owl:ObjectProperty ;
    rdfs:label "has commit" ;
    rdfs:comment "Links repository or branch to commits" ;
    rdfs:domain vcs:Repository ;
    rdfs:range vcs:Commit ;
    IAO:0000115 "Associates a repository or branch with the commits it contains." .

vcs:hasBranch a owl:ObjectProperty ;
    rdfs:label "has branch" ;
    rdfs:comment "Links repository to branches" ;
    rdfs:domain vcs:Repository ;
    rdfs:range vcs:Branch ;
    IAO:0000115 "Associates a repository with the branches it contains." .

vcs:hasTag a owl:ObjectProperty ;
    rdfs:label "has tag" ;
    rdfs:comment "Links repository to tags" ;
    rdfs:domain vcs:Repository ;
    rdfs:range vcs:Tag ;
    IAO:0000115 "Associates a repository with the tags it contains." .

vcs:hasFile a owl:ObjectProperty ;
    rdfs:label "has file" ;
    rdfs:comment "Links commit or repository to files" ;
    rdfs:range vcs:File ;
    IAO:0000115 "Associates a commit or repository with the files it contains or affects." .

vcs:parentCommit a owl:ObjectProperty ;
    rdfs:label "parent commit" ;
    rdfs:comment "Parent commit(s) of this commit" ;
    rdfs:domain vcs:Commit ;
    rdfs:range vcs:Commit ;
    IAO:0000115 "Identifies the preceding commit(s) in the repository history from which this commit was derived." .

vcs:pointsTo a owl:ObjectProperty ;
    rdfs:label "points to" ;
    rdfs:comment "Reference or tag points to a commit" ;
    rdfs:domain vcs:Reference ;
    rdfs:range vcs:Commit ;
    IAO:0000115 "Indicates that a reference or tag points to a specific commit in the repository." .

vcs:authoredBy a owl:ObjectProperty ;
    rdfs:subPropertyOf prov:wasAttributedTo ;
    rdfs:label "authored by" ;
    rdfs:comment "Person who authored the commit" ;
    rdfs:domain vcs:Commit ;
    rdfs:range pkg:Contributor ;
    IAO:0000115 "Identifies the person who originally wrote the changes contained in the commit." .

vcs:committedBy a owl:ObjectProperty ;
    rdfs:subPropertyOf prov:wasAttributedTo ;
    rdfs:label "committed by" ;
    rdfs:comment "Person who committed the changes" ;
    rdfs:domain vcs:Commit ;
    rdfs:range pkg:Contributor ;
    IAO:0000115 "Identifies the person who applied the changes to the repository, which may differ from the original author." .

vcs:affectsFile a owl:ObjectProperty ;
    rdfs:label "affects file" ;
    rdfs:comment "Commit affects these files" ;
    rdfs:domain vcs:Commit ;
    rdfs:range vcs:File ;
    IAO:0000115 "Indicates that a commit modifies, adds, deletes, or otherwise affects the specified file." .

vcs:addedFile a owl:ObjectProperty ;
    rdfs:subPropertyOf vcs:affectsFile ;
    rdfs:label "added file" ;
    rdfs:comment "Files added in this commit" ;
    IAO:0000115 "Indicates files that were newly created and added to the repository in this commit." .

vcs:modifiedFile a owl:ObjectProperty ;
    rdfs:subPropertyOf vcs:affectsFile ;
    rdfs:label "modified file" ;
    rdfs:comment "Files modified in this commit" ;
    IAO:0000115 "Indicates files that had their content changed in this commit." .

vcs:deletedFile a owl:ObjectProperty ;
    rdfs:subPropertyOf vcs:affectsFile ;
    rdfs:label "deleted file" ;
    rdfs:comment "Files deleted in this commit" ;
    IAO:0000115 "Indicates files that were removed from the repository in this commit." .

vcs:renamedFile a owl:ObjectProperty ;
    rdfs:subPropertyOf vcs:affectsFile ;
    rdfs:label "renamed file" ;
    rdfs:comment "Files renamed in this commit" ;
    IAO:0000115 "Indicates files that were moved or renamed in this commit." .

# Repository hosting platforms
vcs:GitHub a owl:NamedIndividual ;
    rdfs:label "GitHub" ;
    rdfs:comment "GitHub hosting platform" .

vcs:GitLab a owl:NamedIndividual ;
    rdfs:label "GitLab" ;
    rdfs:comment "GitLab hosting platform" .

vcs:Bitbucket a owl:NamedIndividual ;
    rdfs:label "Bitbucket" ;
    rdfs:comment "Bitbucket hosting platform" .

vcs:Codeberg a owl:NamedIndividual ;
    rdfs:label "Codeberg" ;
    rdfs:comment "Codeberg hosting platform" .

vcs:SourceHut a owl:NamedIndividual ;
    rdfs:label "SourceHut" ;
    rdfs:comment "SourceHut hosting platform" .

vcs:Savannah a owl:NamedIndividual ;
    rdfs:label "GNU Savannah" ;
    rdfs:comment "GNU Savannah hosting platform" .

# Release and Package Integration
vcs:Release a owl:Class ;
    rdfs:subClassOf vcs:Tag ;
    rdfs:label "Release" ;
    rdfs:comment "A tagged release in version control" ;
    IAO:0000115 "A specific tagged version in version control that represents a formal release of the software, typically corresponding to a published package version." .

vcs:ReleaseCandidate a owl:Class ;
    rdfs:subClassOf vcs:Release ;
    rdfs:label "Release Candidate" ;
    rdfs:comment "A pre-release version for testing" ;
    IAO:0000115 "A version tagged as a candidate for release, used for final testing before the official release." .

vcs:Milestone a owl:Class ;
    rdfs:subClassOf prov:Entity ;
    rdfs:label "Milestone" ;
    rdfs:comment "A development milestone or sprint" ;
    IAO:0000115 "A significant point in the development timeline marking the completion of specific features or goals." .

vcs:ReleaseNotes a owl:Class ;
    rdfs:subClassOf owl:Thing ;
    rdfs:label "Release Notes" ;
    rdfs:comment "Documentation describing changes in a release" ;
    IAO:0000115 "Documentation that describes the changes, bug fixes, and new features included in a software release." .

# Release cadence and timing properties
vcs:releaseFrequency a owl:DatatypeProperty ;
    rdfs:label "release frequency" ;
    rdfs:comment "The typical frequency of releases (in days)" ;
    rdfs:domain vcs:Repository ;
    rdfs:range xsd:int ;
    IAO:0000115 "The average number of days between releases for this repository." .

vcs:releaseDate a owl:DatatypeProperty ;
    rdfs:label "release date" ;
    rdfs:comment "The date when a release was published" ;
    rdfs:domain vcs:Release ;
    rdfs:range xsd:dateTime ;
    IAO:0000115 "The date and time when a release was officially published or made available." .

vcs:isPreRelease a owl:DatatypeProperty ;
    rdfs:label "is pre-release" ;
    rdfs:comment "Whether this is a pre-release version" ;
    rdfs:domain vcs:Release ;
    rdfs:range xsd:boolean ;
    IAO:0000115 "Indicates whether this release is a pre-release, alpha, beta, or release candidate rather than a stable release." .

vcs:releaseStability a owl:DatatypeProperty ;
    rdfs:label "release stability" ;
    rdfs:comment "The stability level of the release (alpha, beta, rc, stable)" ;
    rdfs:domain vcs:Release ;
    rdfs:range xsd:string ;
    IAO:0000115 "Indicates the stability level of the release, such as alpha, beta, release candidate, or stable." .

vcs:developmentStatus a owl:DatatypeProperty ;
    rdfs:label "development status" ;
    rdfs:comment "Current development status of the repository" ;
    rdfs:domain vcs:Repository ;
    rdfs:range xsd:string ;
    IAO:0000115 "Indicates the current development status such as active, maintenance, archived, or deprecated." .

# Package-VCS integration properties
vcs:correspondingPackageVersion a owl:ObjectProperty ;
    rdfs:label "corresponding package version" ;
    rdfs:comment "Links a VCS release to the corresponding package version" ;
    rdfs:domain vcs:Release ;
    IAO:0000115 "Associates a VCS release with the package version that was built from this release." .

vcs:hasUpstreamRepository a owl:ObjectProperty ;
    rdfs:label "has upstream repository" ;
    rdfs:comment "Links a package to its upstream source repository" ;
    rdfs:range vcs:Repository ;
    IAO:0000115 "Associates a package with the upstream version control repository where its source code is maintained." .

vcs:packagedFromTag a owl:ObjectProperty ;
    rdfs:label "packaged from tag" ;
    rdfs:comment "Indicates which VCS tag a package was built from" ;
    rdfs:range vcs:Tag ;
    IAO:0000115 "Associates a package with the specific VCS tag or release from which it was built." .

vcs:packagedFromCommit a owl:ObjectProperty ;
    rdfs:label "packaged from commit" ;
    rdfs:comment "Indicates which commit a package was built from" ;
    rdfs:range vcs:Commit ;
    IAO:0000115 "Associates a package with the specific commit from which it was built." .

# Release relationship properties
vcs:previousRelease a owl:ObjectProperty ;
    rdfs:label "previous release" ;
    rdfs:comment "Links to the previous release in sequence" ;
    rdfs:domain vcs:Release ;
    rdfs:range vcs:Release ;
    IAO:0000115 "Identifies the previous release in the chronological sequence of releases." .

vcs:nextRelease a owl:ObjectProperty ;
    owl:inverseOf vcs:previousRelease ;
    rdfs:label "next release" ;
    rdfs:comment "Links to the next release in sequence" ;
    rdfs:domain vcs:Release ;
    rdfs:range vcs:Release ;
    IAO:0000115 "Identifies the next release in the chronological sequence of releases." .

vcs:hasReleaseNotes a owl:ObjectProperty ;
    rdfs:label "has release notes" ;
    rdfs:comment "Links a release to its documentation" ;
    rdfs:domain vcs:Release ;
    rdfs:range vcs:ReleaseNotes ;
    IAO:0000115 "Associates a release with its accompanying release notes or changelog documentation." .

vcs:releasedBy a owl:ObjectProperty ;
    rdfs:label "released by" ;
    rdfs:comment "Person or organization who published the release" ;
    rdfs:domain vcs:Release ;
    IAO:0000115 "Identifies the person or organization responsible for publishing and announcing the release." .

# Milestone and planning properties
vcs:hasTarget a owl:ObjectProperty ;
    rdfs:label "has target" ;
    rdfs:comment "Links milestone to target release" ;
    rdfs:domain vcs:Milestone ;
    rdfs:range vcs:Release ;
    IAO:0000115 "Associates a development milestone with its target release." .

vcs:targetDate a owl:DatatypeProperty ;
    rdfs:label "target date" ;
    rdfs:comment "Target completion date for milestone" ;
    rdfs:domain vcs:Milestone ;
    rdfs:range xsd:dateTime ;
    IAO:0000115 "The planned or target completion date for a milestone or release." .

vcs:milestoneProgress a owl:DatatypeProperty ;
    rdfs:label "milestone progress" ;
    rdfs:comment "Completion percentage of milestone (0.0 to 1.0)" ;
    rdfs:domain vcs:Milestone ;
    rdfs:range xsd:float ;
    IAO:0000115 "The current completion progress of a milestone as a decimal value between 0.0 and 1.0." .

# Enhanced security and signing
vcs:isSignedRelease a owl:DatatypeProperty ;
    rdfs:label "is signed release" ;
    rdfs:comment "Whether the release is cryptographically signed" ;
    rdfs:domain vcs:Release ;
    rdfs:range xsd:boolean ;
    IAO:0000115 "Indicates whether the release has been cryptographically signed for integrity verification." .

vcs:signatureKeyId a owl:DatatypeProperty ;
    rdfs:label "signature key ID" ;
    rdfs:comment "The key ID used to sign the release" ;
    rdfs:domain vcs:Release ;
    rdfs:range xsd:string ;
    IAO:0000115 "The identifier of the cryptographic key used to sign the release." .

vcs:signatureAlgorithm a owl:DatatypeProperty ;
    rdfs:label "signature algorithm" ;
    rdfs:comment "The cryptographic algorithm used for signing" ;
    rdfs:domain vcs:Release ;
    rdfs:range xsd:string ;
    IAO:0000115 "The cryptographic algorithm used to create the release signature." .

# Repository activity metrics
vcs:lastActivity a owl:DatatypeProperty ;
    rdfs:label "last activity" ;
    rdfs:comment "Date of last commit or activity" ;
    rdfs:domain vcs:Repository ;
    rdfs:range xsd:dateTime ;
    IAO:0000115 "The date and time of the most recent commit or activity in the repository." .

vcs:totalCommits a owl:DatatypeProperty ;
    rdfs:label "total commits" ;
    rdfs:comment "Total number of commits in repository" ;
    rdfs:domain vcs:Repository ;
    rdfs:range xsd:int ;
    IAO:0000115 "The total number of commits made to the repository throughout its history." .

vcs:commitFrequency a owl:DatatypeProperty ;
    rdfs:label "commit frequency" ;
    rdfs:comment "Average commits per day over recent period" ;
    rdfs:domain vcs:Repository ;
    rdfs:range xsd:float ;
    IAO:0000115 "The average number of commits per day over a recent time period, indicating development activity level." .

vcs:contributorCount a owl:DatatypeProperty ;
    rdfs:label "contributor count" ;
    rdfs:comment "Number of unique contributors" ;
    rdfs:domain vcs:Repository ;
    rdfs:range xsd:int ;
    IAO:0000115 "The total number of unique contributors who have made commits to the repository." .

# VCS Types
vcs:Git a owl:NamedIndividual ;
    rdfs:label "Git" ;
    rdfs:comment "Git version control system" .

vcs:Subversion a owl:NamedIndividual ;
    rdfs:label "Subversion" ;
    rdfs:comment "Apache Subversion version control system" .

vcs:Mercurial a owl:NamedIndividual ;
    rdfs:label "Mercurial" ;
    rdfs:comment "Mercurial version control system" .

vcs:Bazaar a owl:NamedIndividual ;
    rdfs:label "Bazaar" ;
    rdfs:comment "Bazaar version control system" .

vcs:CVS a owl:NamedIndividual ;
    rdfs:label "CVS" ;
    rdfs:comment "Concurrent Versions System" .

vcs:Fossil a owl:NamedIndividual ;
    rdfs:label "Fossil" ;
    rdfs:comment "Fossil distributed version control system" .

# Cardinality Restrictions for VCS Classes
vcs:Commit rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty vcs:commitHash ;
    owl:cardinality 1
] .

vcs:Commit rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty vcs:commitTimestamp ;
    owl:cardinality 1
] .

vcs:Tag rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty vcs:tagName ;
    owl:cardinality 1
] .

vcs:Branch rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty vcs:branchName ;
    owl:cardinality 1
] .

vcs:Repository rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty vcs:defaultBranch ;
    owl:maxCardinality 1
] .
