# RPM Package Ontology
@prefix rpm: <https://packagegraph.github.io/ontology/rpm#> .
@prefix pkg: <https://packagegraph.github.io/ontology/core#> .
@prefix vcs: <https://packagegraph.github.io/ontology/vcs#> .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix spdx: <http://spdx.org/spdxdocs/spdx-v2.2.1#> .
@prefix IAO: <http://purl.obolibrary.org/obo/IAO_> .


# Ontology Declaration
<https://packagegraph.github.io/ontology/rpm#> a owl:Ontology ;
    rdfs:label "RPM Package Ontology" ;
    rdfs:comment "Ontology for representing RPM packages and YUM/DNF repositories" ;
    dcterms:created "2025-09-05"^^xsd:date ;
    dcterms:creator "Package Management Ontology Project" ;
    dcterms:title "RPM Package Ontology" ;
    dcterms:description "A comprehensive ontology for modeling RPM packages, YUM/DNF repositories, NEVR identifiers, and RPM-specific metadata including build information, changelogs, and file attributes." ;
    dcterms:license <https://creativecommons.org/publicdomain/zero/1.0/> ;
    owl:versionInfo "0.2.0" ;
    owl:imports <core.ttl> , <vcs.ttl> .

# RPM-specific Classes
rpm:RPMPackage a owl:Class ;
    rdfs:subClassOf pkg:Package ;
    rdfs:label "RPM Package" ;
    rdfs:comment "A package in RPM format" ;
    IAO:0000115 "A software package in Red Hat Package Manager (RPM) format that can be managed by RPM-based package managers like YUM, DNF, or Zypper." .

rpm:SourceRPM a owl:Class ;
    rdfs:subClassOf rpm:RPMPackage ;
    rdfs:label "Source RPM" ;
    rdfs:comment "An RPM source package (SRPM) containing source code and spec file" ;
    IAO:0000115 "An RPM source package containing source code, patches, and a spec file with build instructions used to create one or more binary RPM packages." .

rpm:BinaryRPM a owl:Class ;
    rdfs:subClassOf rpm:RPMPackage ;
    rdfs:label "Binary RPM" ;
    rdfs:comment "A binary RPM package containing compiled software" ;
    IAO:0000115 "A compiled RPM package containing executable binaries, libraries, configuration files, and metadata that can be installed using YUM, DNF, or rpm command." .

rpm:RPMRepository a owl:Class ;
    rdfs:subClassOf pkg:Repository ;
    rdfs:label "RPM Repository" ;
    rdfs:comment "A repository containing RPM packages accessible via YUM/DNF" ;
    IAO:0000115 "A repository containing RPM packages that can be accessed and managed through YUM, DNF, or other RPM-based package managers." .

rpm:RPMGroup a owl:Class ;
    rdfs:subClassOf owl:Thing ;
    rdfs:label "RPM Group" ;
    rdfs:comment "A categorical grouping of RPM packages (e.g., Development/Tools)" ;
    IAO:0000115 "A hierarchical classification system for organizing RPM packages by functionality or purpose, such as Applications, Development, or System Environment." .

rpm:NEVR a owl:Class ;
    rdfs:subClassOf owl:Thing ;
    rdfs:label "NEVR" ;
    rdfs:comment "Name-Epoch-Version-Release identifier for RPM packages" ;
    IAO:0000115 "A unique identifier for RPM packages consisting of Name, Epoch, Version, and Release components that determines package ordering and upgrade relationships." .

rpm:RPMTag a owl:Class ;
    rdfs:subClassOf owl:Thing ;
    rdfs:label "RPM Tag" ;
    rdfs:comment "A metadata tag in an RPM header" ;
    IAO:0000115 "A metadata field in the RPM package header that stores specific information about the package, such as name, version, dependencies, or file attributes." .

rpm:Changelog a owl:Class ;
    rdfs:subClassOf owl:Thing ;
    rdfs:label "Changelog" ;
    rdfs:comment "A changelog entry for an RPM package" ;
    IAO:0000115 "A record of changes made to an RPM package, including version updates, bug fixes, and feature additions, with associated timestamps and author information." .

rpm:Dependency a owl:Class ;
    rdfs:subClassOf pkg:Dependency ;
    rdfs:label "RPM Dependency" ;
    rdfs:comment "A dependency relationship in RPM, potentially with version constraints" ;
    IAO:0000115 "Represents a dependency relationship between RPM packages, which can include version constraints and flags." .

# RPM-specific Properties
rpm:epoch a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:label "epoch" ;
    rdfs:comment "The epoch version number (optional, defaults to 0)" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:int ;
    IAO:0000115 "An optional integer that allows package maintainers to force a version ordering when the normal version comparison would be incorrect." .

rpm:release a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:label "release" ;
    rdfs:comment "The release number of the package" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:string ;
    IAO:0000115 "Specifies the release number of the package, used to distinguish between different builds of the same version." .

rpm:buildHost a owl:DatatypeProperty ;
    rdfs:label "build host" ;
    rdfs:comment "The hostname where the package was built" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:string ;
    IAO:0000115 "Specifies the hostname of the machine where the RPM package was compiled and built." .

rpm:buildTime a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:label "build time" ;
    rdfs:comment "The timestamp when the package was built" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:dateTime ;
    IAO:0000115 "Records the date and time when the RPM package was compiled and built." .

rpm:vendor a owl:DatatypeProperty ;
    rdfs:label "vendor" ;
    rdfs:comment "The vendor of the package" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:string ;
    IAO:0000115 "Identifies the vendor or organization that distributed this RPM package." .

rpm:packager a owl:DatatypeProperty ;
    rdfs:label "packager" ;
    rdfs:comment "The person or entity who packaged the software" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:string ;
    IAO:0000115 "Identifies the person or organization responsible for creating the RPM package, typically including name and email address." .

rpm:distribution a owl:DatatypeProperty ;
    rdfs:label "distribution" ;
    rdfs:comment "The distribution name (e.g., Fedora, RHEL)" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:string ;
    IAO:0000115 "Identifies the Linux distribution for which this RPM package was built, such as Fedora, Red Hat Enterprise Linux, or CentOS." .

rpm:distTag a owl:DatatypeProperty ;
    rdfs:label "distribution tag" ;
    rdfs:comment "The distribution tag (e.g., fc35, el8)" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:string ;
    IAO:0000115 "A short identifier indicating the specific distribution version for which the package was built, such as fc35 for Fedora 35 or el8 for RHEL 8." .

rpm:sourceRPM a owl:DatatypeProperty ;
    rdfs:label "source RPM" ;
    rdfs:comment "The filename of the source RPM this package was built from" ;
    rdfs:domain rpm:BinaryRPM ;
    rdfs:range xsd:string ;
    IAO:0000115 "Specifies the filename of the source RPM package from which this binary RPM was built." .

rpm:rpmVersion a owl:DatatypeProperty ;
    rdfs:label "RPM version" ;
    rdfs:comment "The version of RPM used to build the package" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:string ;
    IAO:0000115 "Specifies the version of the RPM package manager that was used to create this package." .

rpm:payloadFormat a owl:DatatypeProperty ;
    rdfs:label "payload format" ;
    rdfs:comment "The format of the package payload (typically cpio)" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:string ;
    IAO:0000115 "Specifies the archive format used for the package payload, typically cpio." .

rpm:payloadCompressor a owl:DatatypeProperty ;
    rdfs:label "payload compressor" ;
    rdfs:comment "The compression method used for the payload" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:string ;
    IAO:0000115 "Specifies the compression algorithm used to compress the package payload, such as gzip, bzip2, or xz." .

rpm:cookie a owl:DatatypeProperty, owl:FunctionalProperty ;
    rdfs:label "cookie" ;
    rdfs:comment "An opaque identifier for tracking packages from the same build" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:string ;
    IAO:0000115 "An opaque identifier used to group packages that were built together in the same build session or batch." .

rpm:platform a owl:DatatypeProperty ;
    rdfs:label "platform" ;
    rdfs:comment "The platform string (arch-os-vendor)" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:string ;
    IAO:0000115 "Specifies the target platform for the package in the format architecture-operating_system-vendor." .

rpm:optFlags a owl:DatatypeProperty ;
    rdfs:label "optimization flags" ;
    rdfs:comment "The compiler optimization flags used during build" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:string ;
    IAO:0000115 "Specifies the compiler optimization flags that were used when building the software in this RPM package." .

rpm:changelogText a owl:DatatypeProperty ;
    rdfs:label "changelog text" ;
    rdfs:comment "The text content of a changelog entry" ;
    rdfs:domain rpm:Changelog ;
    rdfs:range xsd:string ;
    IAO:0000115 "Contains the descriptive text of a changelog entry detailing the changes made in a package version." .

rpm:changelogAuthor a owl:DatatypeProperty ;
    rdfs:label "changelog author" ;
    rdfs:comment "The author of a changelog entry" ;
    rdfs:domain rpm:Changelog ;
    rdfs:range xsd:string ;
    IAO:0000115 "Identifies the person who authored a specific changelog entry, typically including name and email address." .

rpm:changelogTime a owl:DatatypeProperty ;
    rdfs:label "changelog time" ;
    rdfs:comment "The timestamp of a changelog entry" ;
    rdfs:domain rpm:Changelog ;
    rdfs:range xsd:dateTime ;
    IAO:0000115 "Records the date and time when a changelog entry was created or when the associated changes were made." .

# Object Properties
rpm:inGroup a owl:ObjectProperty ;
    rdfs:label "in group" ;
    rdfs:comment "Links a package to its RPM group" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range rpm:RPMGroup ;
    IAO:0000115 "Associates an RPM package with its functional group classification." .

rpm:hasNEVR a owl:ObjectProperty ;
    rdfs:label "has NEVR" ;
    rdfs:comment "Links a package to its NEVR identifier" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range rpm:NEVR ;
    IAO:0000115 "Associates an RPM package with its Name-Epoch-Version-Release identifier." .

rpm:hasChangelog a owl:ObjectProperty ;
    rdfs:label "has changelog" ;
    rdfs:comment "Links a package to its changelog entries" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range rpm:Changelog ;
    IAO:0000115 "Associates an RPM package with its changelog entries that document the history of changes." .

rpm:hasTag a owl:ObjectProperty ;
    rdfs:label "has tag" ;
    rdfs:comment "Links a package to its RPM tags" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range rpm:RPMTag ;
    IAO:0000115 "Associates an RPM package with its metadata tags stored in the package header." .

# Cross-format property equivalences are declared within property definitions below

# Link to core VCS and ecosystem concepts  
rpm:RPMPackage rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty pkg:partOfDistribution ;
    owl:someValuesFrom pkg:Distribution
] .

rpm:RPMRepository rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty vcs:hasUpstreamRepository ;
    owl:someValuesFrom vcs:Repository
] .

# Enhanced RPM-specific analysis properties
rpm:rpmSignature a owl:DatatypeProperty ;
    rdfs:label "RPM signature" ;
    rdfs:comment "The cryptographic signature of the RPM package" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:string ;
    IAO:0000115 "The cryptographic signature used to verify the authenticity and integrity of the RPM package." .

rpm:rpmSignatureKeyId a owl:DatatypeProperty ;
    rdfs:label "RPM signature key ID" ;
    rdfs:comment "The key ID used to sign the RPM package" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:string ;
    IAO:0000115 "The identifier of the GPG key used to sign the RPM package." .

rpm:buildArch a owl:DatatypeProperty ;
    rdfs:label "build architecture" ;
    rdfs:comment "The architecture this RPM was built on" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:string ;
    IAO:0000115 "The architecture of the build system where this RPM package was compiled." .

rpm:installTime a owl:DatatypeProperty ;
    rdfs:label "install time" ;
    rdfs:comment "When this package was installed (if applicable)" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:dateTime ;
    IAO:0000115 "The date and time when this RPM package was installed on a system." .

rpm:relocatable a owl:DatatypeProperty ;
    rdfs:label "relocatable" ;
    rdfs:comment "Whether the package can be relocated during installation" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:boolean ;
    IAO:0000115 "Indicates whether the RPM package can be installed to a different location than its default prefix." .

# Enhanced NEVR analysis
rpm:nevrString a owl:DatatypeProperty ;
    rdfs:label "NEVR string" ;
    rdfs:comment "The complete NEVR string representation" ;
    rdfs:domain rpm:NEVR ;
    rdfs:range xsd:string ;
    IAO:0000115 "The complete Name-Epoch-Version-Release string in standard RPM format." .

rpm:versionComparison a owl:DatatypeProperty ;
    rdfs:label "version comparison" ;
    rdfs:comment "Numeric value for version comparison ordering" ;
    rdfs:domain rpm:NEVR ;
    rdfs:range xsd:long ;
    IAO:0000115 "A numeric representation that allows for proper version ordering and comparison of NEVR identifiers." .

# Repository and distribution enhancements
rpm:repomd a owl:DatatypeProperty ;
    rdfs:label "repomd" ;
    rdfs:comment "Repository metadata (repomd.xml) content" ;
    rdfs:domain rpm:RPMRepository ;
    rdfs:range xsd:string ;
    IAO:0000115 "The repository metadata in repomd.xml format that describes the repository structure and contents." .

rpm:gpgKeyUrl a owl:DatatypeProperty ;
    rdfs:label "GPG key URL" ;
    rdfs:comment "URL to the repository's GPG signing key" ;
    rdfs:domain rpm:RPMRepository ;
    rdfs:range xsd:anyURI ;
    IAO:0000115 "The URL where the GPG public key for verifying repository packages can be downloaded." .

rpm:mirrorList a owl:DatatypeProperty ;
    rdfs:label "mirror list" ;
    rdfs:comment "URL to the list of repository mirrors" ;
    rdfs:domain rpm:RPMRepository ;
    rdfs:range xsd:anyURI ;
    IAO:0000115 "The URL that provides a list of mirror servers for this repository." .

# Dependency Properties
rpm:requires a owl:ObjectProperty ;
    rdfs:subPropertyOf pkg:dependsOn ;
    owl:equivalentProperty pkg:rpmRequires ;
    rdfs:label "requires" ;
    rdfs:comment "Runtime dependency relationship" ;
    IAO:0000115 "Specifies packages or capabilities that must be available for this RPM package to function properly at runtime." .

rpm:provides a owl:ObjectProperty ;
    rdfs:subPropertyOf pkg:provides ;
    owl:equivalentProperty pkg:rpmProvides ;
    rdfs:label "provides" ;
    rdfs:comment "Virtual package provision" ;
    IAO:0000115 "Indicates that this RPM package provides the functionality of another package or virtual package name." .

rpm:obsoletes a owl:ObjectProperty ;
    rdfs:subPropertyOf pkg:replaces ;
    rdfs:label "makes obsolete" ;
    rdfs:comment "Package obsolescence relationship" ;
    IAO:0000115 "Indicates that this RPM package makes another package obsolete and should replace it during upgrades." .

rpm:conflicts a owl:ObjectProperty ;
    rdfs:subPropertyOf pkg:conflicts ;
    owl:equivalentProperty pkg:rpmConflicts ;
    rdfs:label "conflicts" ;
    rdfs:comment "Package conflict relationship" ;
    IAO:0000115 "Indicates that this RPM package conflicts with another package and they cannot be installed simultaneously." .

rpm:buildRequires a owl:ObjectProperty ;
    rdfs:label "build requires" ;
    rdfs:comment "Build-time dependency relationship" ;
    rdfs:domain rpm:SourceRPM ;
    rdfs:range rpm:RPMPackage ;
    IAO:0000115 "Specifies packages that must be installed to build this source RPM package." .

rpm:onPackage a owl:ObjectProperty ;
    rdfs:label "on package" ;
    rdfs:comment "Links a dependency relationship to the target package" ;
    rdfs:domain rpm:Dependency ;
    rdfs:range rpm:RPMPackage ;
    IAO:0000115 "The target package of a dependency relationship." .

rpm:versionConstraint a owl:DatatypeProperty ;
    rdfs:label "version constraint" ;
    rdfs:comment "A version constraint for a dependency (e.g., '>= 1.2.3')" ;
    rdfs:domain rpm:Dependency ;
    rdfs:range xsd:string ;
    IAO:0000115 "A string specifying the version constraint for a dependency, such as '>= 1.2.3', '< 2.0', or '= 1.5'." .

# File Properties
rpm:fileName a owl:DatatypeProperty ;
    rdfs:label "file name" ;
    rdfs:comment "Name of a file contained in the package" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:string ;
    IAO:0000115 "Specifies the full path and name of a file contained within the RPM package." .

rpm:fileMode a owl:DatatypeProperty ;
    rdfs:label "file mode" ;
    rdfs:comment "Unix file permissions mode" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:int ;
    IAO:0000115 "Specifies the Unix file permissions (mode bits) for a file contained in the RPM package." .

rpm:fileSize a owl:DatatypeProperty ;
    rdfs:label "file size" ;
    rdfs:comment "Size of a file in bytes" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:long ;
    IAO:0000115 "Specifies the size in bytes of a file contained in the RPM package." .

rpm:fileDigest a owl:DatatypeProperty ;
    rdfs:label "file digest" ;
    rdfs:comment "Cryptographic digest of a file" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:string ;
    IAO:0000115 "A cryptographic hash (typically MD5, SHA1, or SHA256) of a file contained in the RPM package for integrity verification." .

rpm:fileOwner a owl:DatatypeProperty ;
    rdfs:label "file owner" ;
    rdfs:comment "Unix owner of a file" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:string ;
    IAO:0000115 "Specifies the Unix user ownership of a file contained in the RPM package." .

rpm:fileGroup a owl:DatatypeProperty ;
    rdfs:label "file group" ;
    rdfs:comment "Unix group of a file" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range xsd:string ;
    IAO:0000115 "Specifies the Unix group ownership of a file contained in the RPM package." .

# RPM-specific Activities
rpm:RPMBuildActivity a owl:Class ;
    rdfs:subClassOf pkg:BuildActivity ;
    rdfs:label "RPM Build Activity" ;
    rdfs:comment "The process of building an RPM package using rpmbuild" ;
    IAO:0000115 "An activity that uses rpmbuild to compile source code and create an RPM package from a spec file and source materials." .

rpm:SpecFileProcessing a owl:Class ;
    rdfs:subClassOf prov:Activity ;
    rdfs:label "Spec File Processing" ;
    rdfs:comment "The process of parsing and executing an RPM spec file" ;
    IAO:0000115 "An activity that parses an RPM spec file and executes its build instructions to create an RPM package." .

rpm:SRPMCreation a owl:Class ;
    rdfs:subClassOf pkg:PackagingActivity ;
    rdfs:label "SRPM Creation" ;
    rdfs:comment "The process of creating a source RPM package" ;
    IAO:0000115 "An activity that packages source code, patches, and spec file into a source RPM (SRPM) package." .

# RPM-specific provenance properties
rpm:builtFromSRPM a owl:ObjectProperty ;
    rdfs:subPropertyOf prov:wasGeneratedBy ;
    rdfs:label "built from SRPM" ;
    rdfs:comment "Links a binary RPM to the SRPM creation activity that produced its source" ;
    rdfs:domain rpm:BinaryRPM ;
    rdfs:range rpm:SRPMCreation ;
    IAO:0000115 "Associates a binary RPM with the SRPM creation activity that produced the source package from which it was built." .

rpm:usedSpecFile a owl:ObjectProperty ;
    rdfs:subPropertyOf prov:used ;
    rdfs:label "used spec file" ;
    rdfs:comment "Links an RPM build activity to the spec file it used" ;
    rdfs:domain rpm:RPMBuildActivity ;
    IAO:0000115 "Associates an RPM build activity with the spec file that contained the build instructions." .

rpm:generatedBy a owl:ObjectProperty ;
    rdfs:subPropertyOf prov:wasGeneratedBy ;
    rdfs:label "generated by" ;
    rdfs:comment "Links an RPM package to the build activity that created it" ;
    rdfs:domain rpm:RPMPackage ;
    rdfs:range rpm:RPMBuildActivity ;
    IAO:0000115 "Associates an RPM package with the specific build activity that created it using rpmbuild." .

# Cardinality Restrictions for RPM Classes
rpm:RPMPackage rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty rpm:hasNEVR ;
    owl:cardinality 1
] .

rpm:RPMPackage rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty rpm:buildTime ;
    owl:cardinality 1
] .

rpm:RPMPackage rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty rpm:release ;
    owl:cardinality 1
] .

rpm:BinaryRPM rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty rpm:sourceRPM ;
    owl:cardinality 1
] .

# Common RPM Groups
rpm:Applications a rpm:RPMGroup ;
    rdfs:label "Applications" ;
    rdfs:comment "Application software group" .

rpm:Development a rpm:RPMGroup ;
    rdfs:label "Development" ;
    rdfs:comment "Development tools and libraries" .

rpm:System a rpm:RPMGroup ;
    rdfs:label "System Environment" ;
    rdfs:comment "System environment software" .

rpm:Documentation a rpm:RPMGroup ;
    rdfs:label "Documentation" ;
    rdfs:comment "Documentation files" .
